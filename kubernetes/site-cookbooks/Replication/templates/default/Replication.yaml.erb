apiVersion: <%= @apiVersion %>
kind: <%= @kind %>
metadata:
  name: <%= @name %>
spec:
  replicas: <%= @replicas %>
  selector:
    <%= @selector_key %>: <%= @selector_value %>
  template:
    metadata:
      name: <%= @template_metadata_name %>
      labels:
        <%= @template_metadata_labels_key %>: <%= @template_metadata_labels_value %>
    spec:
      containers:
      - name: <%= @container_name %>
        image: <%= @container_image %>
        ports:
        - containerPort: <%= @container_port %>
